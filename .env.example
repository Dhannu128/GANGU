# GANGU Environment Variables Template
# Copy this to .env and fill in your actual values

# Required: Google Gemini API Key
GEMINI_API_KEY=your-gemini-key-here

# Optional: LangSmith for tracing (set to false if not using)
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY=your-langsmith-key-here
LANGSMITH_PROJECT="GANGU"

# Optional: Zepto MCP Configuration
ZEPTO_PHONE_NUMBER=your-phone-here
ZEPTO_DEFAULT_ADDRESS=your-address-here

# MongoDB (uses default if not set)
MONGODB_URI=mongodb://localhost:27017
